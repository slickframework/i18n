{
  "name": "slick/i18n",
  "description": "Internationalization and string translation package Slick Framework",
  "license": "MIT",
  "minimum-stability": "stable",
  "homepage": "https://github.com/slickframework/tempalte",
  "keywords": ["slick", "i18n", "translation"],
  "version": "v1.3.1",
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Slick\\I18n\\": "src"
    }
  },
  "require": {
    "zendframework/zend-i18n": "^2.7@dev",
    "zendframework/zend-servicemanager": "^3.3@dev"
  },
  "require-dev": {
    "phpspec/phpspec": "^4.0@alpha",
    "squizlabs/php_codesniffer": "^3.0@dev",
    "behat/behat": "^3.2@dev"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.3-dev",
      "dev-develop": "1.4-dev"
    }
  },
  "scripts": {
    "test": "phpspec run -fdot",
    "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
    "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
  }
}
