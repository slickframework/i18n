{
  "name": "slick/i18n",
  "description": "Internationalization and string translation package Slick Framework",
  "license": "MIT",
  "homepage": "https://github.com/slickframework/tempalte",
  "keywords": ["slick", "i18n", "translation"],
  "version": "v1.2.0",
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
      "role": "Developer"
    }
